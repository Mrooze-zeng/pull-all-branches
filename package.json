    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "eslint-plugin-json": "^3.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  }
